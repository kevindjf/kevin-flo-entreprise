<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- pressed -->
    <item android:state_pressed="true">
        <bitmap android:src="@drawable/autotheque_logo_bleu"/>
    </item>
    <!-- focused -->
    <item android:state_focused="true"> <bitmap android:src="@drawable/autotheque_logo_bleu"/>
    </item>
    <!-- checked -->
    <item android:state_checked="true"> <bitmap android:src="@drawable/autotheque_logo_bleu"/>
    </item>
    <!-- default -->
    <item android:drawable="@drawable/autotheque_logo"/>

</selector>